<?xml version="1.0" ?>
<wdb2ts>
    <querydefs>

        <!-- PROFF + EC.DET -->
        <querydef id="proffecdetforecast" paralell="4">
            <!-- ECMWF felt -->
            <query>
                dataprovider=ecmwf atmospheric;
                point_interpolation=bilinear;
                parameter="geopotential";
                levelspec=0,0,height above ground,exact
            </query>
            <query>
                dataprovider=ecmwf atmospheric;
                point_interpolation=nearest;
                parameter="air pressure at sea level"
                ,"cloud area fraction"
                ,"cloud area fraction in atmosphere layer"
                ,"relative humidity"
                ,"x wind"
                ,"y wind"
                ,"precipitation amount";
            </query>
            <query>
                dataprovider=ecmwf atmospheric;
                point_interpolation=bilinear;
                parameter="air temperature"
                ,"dew point temperature";
            </query>

            <query>
                dataprovider=proff.approved;
                point_interpolation=bilinear;
                reftime=${proff.approved};
                parameter="altitude";
                levelspec=0,0,height above ground,exact
            </query>
            <query>
                dataprovider=proff.approved;
                point_interpolation=nearest;
                reftime=${proff.approved};
                parameter="air pressure at sea level"
                ,"cloud area fraction"
                ,"cloud area fraction in atmosphere layer"
                ,"relative humidity"
                ,"x wind"
                ,"y wind"
                ,"lwe thickness of precipitation amount"
                ,"local 20th percentile of precipitation amount"
                ,"local 50th percentile of precipitation amount"
                ,"local 80th percentile of precipitation amount"
                ,"probability of precipitation"
                ,"thunderstorm probability";
            </query>
            <query stop_if_data="true">  <!-- Dette er den siste queryen hvis det er data i proff.approved -->
                dataprovider=proff.approved;
                point_interpolation=bilinear;
                parameter="air temperature"
            </query>

            <!-- proff.default -->

            <query>
                dataprovider=proff.default;
                point_interpolation=bilinear;
                reftime=${proff.default};
                parameter="altitude";
                levelspec=0,0,height above ground,exact
            </query>
            <query>
                dataprovider=proff.default;
                point_interpolation=nearest;
                reftime=${proff.default};
                parameter="air pressure at sea level"
                ,"cloud area fraction"
                ,"cloud area fraction in atmosphere layer"
                ,"relative humidity"
                ,"x wind"
                ,"y wind"
                ,"lwe thickness of precipitation amount"
                ,"local 20th percentile of precipitation amount"
                ,"local 50th percentile of precipitation amount"
                ,"local 80th percentile of precipitation amount"
                ,"probability of precipitation"
                ,"thunderstorm probability";
            </query>
            <query>
                dataprovider=proff.default;
                point_interpolation=bilinear;
                parameter="air temperature"
            </query>

        </querydef>

        <!-- PROFF + ECEPS -->
        <querydef id="proffecepsforecast" paralell="4">
            <query>
                dataprovider=statkart.no;
                reftime=NULL;   <!-- NB NB NB NB -->
                validtime=NULL; <!-- NB NB NB NB -->
                parameter="altitude";
            </query>

            <query>
                dataprovider=yr corrected;
                validtime=NULL;
                parameter="altitude",
                "air temperature";
            </query>
            <!-- ECEPS -->
            <query>
                dataprovider=met.no eceps modification;
                point_interpolation=bilinear;
                parameter="altitude";
                levelspec=0,0,height above ground,exact
            </query>
            <query>
                dataprovider=pgen_probability;
                point_interpolation=nearest;
                parameter="air pressure at sea level"
                ,"cloud area fraction"
                <!-- ,"relative humidity" -->
                ,"yr weather symbol"
                ,"probability of yr weather symbol"
                ,"x wind"
                ,"y wind"
                ,"probability of wind speed"
                ,"accumulated precipitation surface density"
                ,"lwe thickness of precipitation amount"
                ,"vessel icing index";
            </query>
            <query>
                dataprovider=pgen_probability;
                point_interpolation=bilinear;
                parameter="probability of air temperature +/-1.5"
                ,"probability of air temperature +/-2.5"
                ,"air temperature";
            </query>

            <query>
                dataprovider=proff.approved;
                point_interpolation=bilinear;
                reftime=${proff.approved};
                parameter="altitude";
            </query>
            <query>
                dataprovider=proff.approved;
                point_interpolation=nearest;
                reftime=${proff.approved};
                parameter="air pressure at sea level"
                ,"cloud area fraction"
                ,"cloud area fraction in atmosphere layer"
                ,"relative humidity"
                ,"x wind"
                ,"y wind"
                ,"lwe thickness of precipitation amount"
                ,"local 20th percentile of precipitation amount"
                ,"local 50th percentile of precipitation amount"
                ,"local 80th percentile of precipitation amount"
                ,"probability of precipitation"
                ,"thunderstorm probability"
                ,"vessel icing index";
            </query>
            <query stop_if_data="true">  <!-- Dette er den siste queryen hvis det er data i proff.approved -->
                dataprovider=proff.approved;
                point_interpolation=bilinear;
                parameter="air temperature"
            </query>

            <!-- proff.default -->

            <query>
                dataprovider=proff.default;
                point_interpolation=bilinear;
                reftime=${proff.default};
                parameter="altitude";
            </query>
            <query>
                dataprovider=proff.default;
                point_interpolation=nearest;
                reftime=${proff.default};
                parameter="air pressure at sea level"
                ,"cloud area fraction"
                ,"cloud area fraction in atmosphere layer"
                ,"relative humidity"
                ,"x wind"
                ,"y wind"
                ,"lwe thickness of precipitation amount"
                ,"local 20th percentile of precipitation amount"
                ,"local 50th percentile of precipitation amount"
                ,"local 80th percentile of precipitation amount"
                ,"probability of precipitation"
                ,"thunderstorm probability"
                ,"vessel icing index";
            </query>
            <query>
                dataprovider=proff.default;
                point_interpolation=bilinear;
                parameter="air temperature"
            </query>

        </querydef>

        <!-- PROBABILITYFORECAST -->
        <querydef id="probabilityforecast">


            <query>
                dataprovider=statkart.no;
                reftime=NULL;   <!-- NB NB NB NB -->
                validtime=NULL; <!-- NB NB NB NB -->
                parameter="altitude";
            </query>

            <query>
                dataprovider=met.no eceps modification;
                point_interpolation=bilinear;
                parameter="altitude";
                levelspec=0,0,height above ground,exact
            </query>
            <query>
                dataprovider=pgen_probability;
                point_interpolation=bilinear;
                parameter="10th percentile of air temperature"
                ,"25th percentile of air    temperature"
                ,"50th percentile of air temperature"
                ,"75th percentile of air temperature"
                ,"90th percentile of air temperature";
            </query>
            <query>
                dataprovider=pgen_probability;
                point_interpolation=nearest;
                parameter="10th percentile of precipitation amount"
                    ,"25th percentile of precipitation amount"
                    ,"50th percentile of precipitation amount"
                    ,"75th percentile of precipitation amount"
                    ,"90th percentile of precipitation amount"
                    ,"probability of precipitation >0.2mm"
                    ,"probability of precipitation >0.5mm"
                    ,"probability of precipitation >1.0mm"
                    ,"probability of precipitation >2.0mm"
                    ,"probability of precipitation >5.0mm";
                reftime=${pgen_probability};
            </query>

        </querydef>
        <querydef id="locationforecast" paralell="3">
            <query>
                dataprovider=statkart.no;
                reftime=NULL;   <!-- NB NB NB NB -->
                validtime=NULL; <!-- NB NB NB NB -->
                parameter="altitude";
            </query>
            <query>
                dataprovider=pgen_probability;
                point_interpolation=bilinear;
                parameter="altitude";
                levelspec=0,0,height above ground,exact
            </query>
            <query>
                dataprovider=pgen_probability;
                point_interpolation=nearest;
                parameter="air pressure at sea level"
                ,"cloud area fraction"
                <!-- ,"relative humidity" -->
                ,"yr weather symbol"
                ,"probability of yr weather symbol"
                ,"x wind"
                ,"y wind"
                ,"probability of wind speed"
                ,"accumulated precipitation surface density"
                ,"lwe thickness of precipitation amount";
            </query>
            <query>
                dataprovider=pgen_probability;
                point_interpolation=bilinear;
                parameter="probability of air temperature +/-1.5"
                ,"probability of air temperature +/-2.5"
                ,"air temperature";
            </query>
        </querydef>
        
        
        <!-- Oceanforecast -->
        <querydef id="oceanforecast-demask" paralell="1">
        
            <!-- WAM (WAVE MODEL) -->
            <query must_have_data="true">
                dataprovider=wam norway (demask);
                point_interpolation=nearest;
                reftime=${wam norway (demask)};
                parameter="sea floor depth below geoid"
                ,"sea ice presence"
                ,"sea surface swell wave maximum period"
                ,"sea surface swell wave maximum to direction"
                ,"sea surface swell wave mean period"
                ,"sea surface swell wave mean to direction"
                ,"sea surface swell wave significant height"
                ,"sea surface wave significant height"
                ,"sea surface wave to direction"
                ,"significant wave period";
            </query>
            
            <!-- Circulation models -->
            <query must_have_data="false">
                dataprovider=nordic (demask);
                point_interpolation=nearest;
                reftime=${nordic (demask)};
                parameter="sea surface height above geoid"
                         ,"sea water temperature"
                         ,"sea water x velocity"
                         ,"sea water y velocity";
            </query>
        </querydef>
        
        <!-- Oceanforecast til HALO -->
        <querydef id="oceanforecast" paralell="1">
        
            <!-- WAM (WAVE MODEL) -->
            <query must_have_data="true">
                dataprovider=wam norway;
                point_interpolation=nearest;
                reftime=${wam norway};
                parameter="sea floor depth below geoid"
                ,"sea ice presence"
                ,"sea surface swell wave maximum period"
                ,"sea surface swell wave maximum to direction"
                ,"sea surface swell wave mean period"
                ,"sea surface swell wave mean to direction"
                ,"sea surface swell wave significant height"
                ,"sea surface wave significant height"
                ,"sea surface wave to direction"
                ,"significant wave period";
            </query>
            
            <!-- Circulation models -->
            <query must_have_data="false">
                dataprovider=nordic;
                point_interpolation=nearest;
                reftime=${nordic};
                parameter="sea surface height above geoid"
                         ,"sea water temperature"
                         ,"sea water x velocity"
                         ,"sea water y velocity";
            </query>
        </querydef>
        
    </querydefs>

    <paramdefs>
        <!-- topografi fra base -->
        <paramdef id="TOPOGRAPHY" provider="statkart.no">
            <valueparameter name="altitude" unit="m" />
            <levelparameter name="height above reference ellipsoid"
                unit="m" from="0" to="0" />
            <value scale="1.0" offset="0.0" null="lt:-410" /> <!-- Optional: default scale="1.0" offset="0.0" -->
            <dataversion version="-1" /> <!-- Optional: default value="-1" -->

        </paramdef>

        <paramdef id="MODEL.TOPOGRAPHY">
            <valueparameter name="altitude" unit="m" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" /> <!-- Optional: default scale="1.0" offset="0.0" -->
            <dataversion version="-1" /> <!-- Optional: default value="-1" -->
        </paramdef>

        <paramdef id="MODEL.TOPOGRAPHY" dataprovider="ecmwf atmospheric">
            <valueparameter name="geopotential" unit="m2/s2" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="0.1" offset="0.0" /> <!-- Optional: default scale="1.0" offset="0.0" -->
            <dataversion version="-1" /> <!-- Optional: default value="-1" -->
        </paramdef>
        <!-- yr corrected -->
        <paramdef id="MODEL.TOPOGRAPHY" dataprovider="yr corrected">
            <valueparameter name="altitude" unit="m" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" null="lt:-420" />
            <dataversion version="-1" />
        </paramdef>
        <paramdef id="T.2M.NO_ADIABATIC_HIGHT_CORRECTION"
            dataprovider="yr corrected">
            <valueparameter name="air temperature" unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="2" to="2" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <!-- ECEPS -->
        <paramdef id="PRECIP.PROBABILITY.5,0MM">
            <valueparameter name="probability of precipitation >5.0mm"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PROBABILITY.2,0MM">
            <valueparameter name="probability of precipitation >2.0mm"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PROBABILITY.1,0MM">
            <valueparameter name="probability of precipitation >1.0mm"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PROBABILITY.0,5MM">
            <valueparameter name="probability of precipitation >0.5mm"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PROBABILITY.0,2MM">
            <valueparameter name="probability of precipitation >0.2mm"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PROBABILITY.0,1MM">
            <valueparameter name="probability of precipitation >0.1mm"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PERCENTILE.10">
            <valueparameter name="10th percentile of precipitation amount"
                unit="kg/m2" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1000.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PERCENTILE.25">
            <valueparameter name="25th percentile of precipitation amount"
                unit="kg/m2" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1000.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PERCENTILE.50">
            <valueparameter name="50th percentile of precipitation amount"
                unit="kg/m2" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1000.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PERCENTILE.75">
            <valueparameter name="75th percentile of precipitation amount"
                unit="kg/m2" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1000.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.PERCENTILE.90">
            <valueparameter name="90th percentile of precipitation amount"
                unit="kg/m2" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1000.0" offset="0.0" />
        </paramdef>
        <paramdef id="SYMBOL">
            <valueparameter name="yr weather symbol" unit="" />
            <levelparameter name="height above ground" unit=""
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="SYMBOL.PROBABILITY">
            <valueparameter name="probability of yr weather symbol"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="WIND.PROBABILITY">
            <valueparameter name="probability of wind speed" unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="T.2M.PROBABILITY.1">
            <valueparameter name="probability of air temperature +/-1.5"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="T.2M.PROBABILITY.2">
            <valueparameter name="probability of air temperature +/-2.5"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="T.2M.PROBABILITY.3">
            <valueparameter name="probability of temperature +/-3"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="T.2M.PROBABILITY.4">
            <valueparameter name="probability of temperature +/-4"
                unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>

        <paramdef id="T.2M.PERCENTILE.10">
            <valueparameter name="10th percentile of air temperature"
                unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="T.2M.PERCENTILE.25">
            <valueparameter name="25th percentile of air temperature"
                unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="T.2M.PERCENTILE.50">
            <valueparameter name="50th percentile of air temperature"
                unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="T.2M.PERCENTILE.75">
            <valueparameter name="75th percentile of air temperature"
                unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="T.2M.PERCENTILE.90">
            <valueparameter name="90th percentile of air temperature"
                unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="PRECIP.ACCUMULATED">
            <valueparameter name="lwe thickness of precipitation amount"
                unit="m" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1000.0" offset="0.0" />
        </paramdef>
        <paramdef id="PRECIP.1H">
            <valueparameter name="lwe thickness of precipitation amount"
                unit="m" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1000.0" offset="0.0" />
        </paramdef>
        <paramdef id="FOG">
            <valueparameter name="cloud area fraction in atmosphere layer"
                unit="%" />
            <levelparameter name="atmosphere sigma coordinate"
                unit="none" from="1000" to="1000" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="TOTAL.CLOUD">
            <valueparameter name=" cloud area fraction" unit="%" />
            <levelparameter name="atmosphere sigma coordinate"
                unit="none" from="1000" to="1000" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="LOW.CLOUD">
            <valueparameter name="cloud area fraction in atmosphere layer"
                unit="%" />
            <levelparameter name="atmosphere sigma coordinate"
                unit="none" from="850" to="850" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="MEDIUM.CLOUD">
            <valueparameter name="cloud area fraction in atmosphere layer"
                unit="%" />
            <levelparameter name="atmosphere sigma coordinate"
                unit="none" from="500" to="500" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="HIGH.CLOUD">
            <valueparameter name="cloud area fraction in atmosphere layer"
                unit="%" />
            <levelparameter name="atmosphere sigma coordinate"
                unit="none" from="300" to="300" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="RH.2M">
            <valueparameter name="relative humidity" unit="%" />
            <levelparameter name="height above ground" unit="m"
                from="2" to="2" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="WIND.U10M">
            <valueparameter name="x wind" unit="m/s" />
            <levelparameter name="height above ground" unit="m"
                from="10" to="10" />
        </paramdef>
        <paramdef id="WIND.V10M">
            <valueparameter name="y wind" unit="m/s" />
            <levelparameter name="height above ground" unit="m"
                from="10" to="10" />
        </paramdef>
        <paramdef id="WIND.U10M" dataprovider="pgen_probability">
            <valueparameter name="x wind" unit="m/s" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
        </paramdef>
        <paramdef id="WIND.V10M" dataprovider="pgen_probability">
            <valueparameter name="y wind" unit="m/s" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
        </paramdef>

        <paramdef id="MSLP">
            <valueparameter name="air pressure at sea level" unit="Pa" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="1.0" offset="0.0" />
        </paramdef>

        <paramdef id="T.2M">
            <valueparameter name="air temperature" unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="2" to="2" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="T.2M.LAND">
            <valueparameter name="air temperature" unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="2" to="2" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="FOG.PROBABILITY">
            <valueparameter name="proportion of fog probability"
                unit="ratio" />
            <levelparameter name="height above ground" unit=""
                from="1000" to="1000" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="T.2M.NO_ADIABATIC_HIGHT_CORRECTION"
            dataprovider="inversion">
            <valueparameter name="air temperature" unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="2" to="2" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>

        <!-- ecmwf felt parameters -->
        <paramdef id="MODEL.TOPOGRAPHY" dataprovider="ecmwf atmospheric">
            <valueparameter name="geopotential height specific energy"
                unit="m2/s2" />
            <levelparameter name="height above ground" unit=""
                from="0" to="0" />
            <value scale="0.1" offset="0.0" /> <!-- Optional: default scale="1.0" offset="0.0" -->
            <dataversion version="-1" /> <!-- Optional: default value="-1" -->
        </paramdef>

        <paramdef id="T.2M" dataprovider="ecmwf atmospheric">
            <valueparameter name="air temperature" unit="K" />
            <levelparameter name="height above ground" unit="m"
                from="2" to="2" />
            <value scale="1.0" offset="-273.15" />
        </paramdef>
        <paramdef id="RH.2M" dataprovider="ecmwf atmospheric">
            <valueparameter name="relative humidity" unit="%" />
            <levelparameter name="isobaric surface" unit="Pa"
                from="1000" to="1000" />
            <value scale="1.0" offset="0.0" />
        </paramdef>
        <paramdef id="MSLP" dataprovider="ecmwf atmospheric">
            <valueparameter name="air pressure at sea level" unit="Pa" />
            <levelparameter name="height above ground" unit="m"
                from="0" to="0" />
            <value scale="0.01" offset="0.0" />
        </paramdef>
        <paramdef id="WIND.U10M" dataprovider="ecmwf atmospheric">
            <valueparameter name="x wind" unit="m/s" />
            <levelparameter name="height above ground" unit=""
                from="0" to="0" />
        </paramdef>
        <paramdef id="WIND.V10M" dataprovider="ecmwf atmospheric">
