<?xml version="1.0" ?>
<wdb2ts>
   <requests>
      <request path="/proffecdetforecastupdate" default_version="1.0"
         wdbdb="default" action="LocationForecastUpdate" queryid="proffecdetforecast">
         <actionparam key="updateid" value="proffecdetforecast" />
         <version version="1.0" action="LocationForecastUpdate"
            queryid="proffecdetforecast" />
      </request>
      <request path="/proffecdetforecast" default_version="-1"
         wdbdb="default" action="LocationForecast3" queryid="proffecdetforecast"
         schema="http://api.met.no/weatherapi/locationforecast/1.9/schema">
         <actionparam key="updateid" value="proffecdetforecast" />
         <actionparam key="loglevels" value="default=warn" />
         <actionparam key="meta_model-ecmwf_atmospheric0125_hourly"
            value="name=EC.GEO.0125;nextrun=08:00,20:00" />
         <actionparam key="expire_rand" value="960" />
         <actionparam key="provider_priority" value="ecmwf_atmospheric0125_hourly" />
         <actionparam key="precipitation" value="multi:3,6" />
         <actionparam key="symbol_conf_file" value="qbSymbols.def" />
         <actionparam key="symbol_provider-ecmwf_atmospheric0125_hourly" value="(3,0,3) (6,0,3) (6,0,6)" />
         <actionparam key="output_params" value="dewpointTemperature" />
         <actionparam key="symbols_enable_thunder" value="May-1/Oct-1" />
         <version version="1.1" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.2" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.3" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.4" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.5" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.6" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.7" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.8" action="LocationForecast3" queryid="proffecdetforecast" />
         <version version="1.9" action="LocationForecast5" queryid="proffecdetforecast" >
            <actionparam key="symbol_conf_file" value="qbSymbols.def" />
            <actionparam key="symbol_provider-ecmwf_atmospheric0125_hourly" value="(1) (2) (3) (6)" />
         </version>
      </request>
   </requests>
   <querydefs>

      <!-- PROFF + EC.DET -->
      <querydef id="proffecdetforecast" paralell="2">
         <!-- ECMWF felt -->
         <query>
            dataprovider="ecmwf_atmospheric0125_hourly";
            point_interpolation=bilinear;
            parameter="geopotential height"
                     ,"min air temperature"
                     ,"max air temperature"
                     ,"air temperature";
         </query>
         <query>
            dataprovider="ecmwf_atmospheric0125_hourly";
            point_interpolation=nearest;
            parameter="air pressure at sea level"
                     ,"cloud area fraction"
                     ,"cloud area fraction in atmosphere layer"
                     ,"dew point temperature"
                     ,"x wind"
                     ,"y wind"
                     ,"precipitation amount";
         </query>
      </querydef>
   </querydefs>
</wdb2ts>
